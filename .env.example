# Cloud Resume Project - Environment Variables Template
# Copy this file to .env and fill in your values (DO NOT commit .env to git)

# AWS Configuration
AWS_REGION=us-east-1
AWS_PROFILE=default

# S3 Configuration  
S3_BUCKET_NAME=your-unique-bucket-name-here

# Terraform State (for remote backend)
TF_STATE_BUCKET=your-terraform-state-bucket
TF_STATE_KEY=cloud-resume/terraform.tfstate

# API Configuration
API_STAGE_NAME=prod

# Development
LOCAL_API_URL=http://localhost:3000/visitor-count

# GitHub Repository (for actions)
GITHUB_REPOSITORY=your-username/cloud-resume-project
